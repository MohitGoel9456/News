{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    // ... other configs, if any
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "tests": [
        "tests/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@apiService/*": [
        "src/apiServices/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@storageService/*": [
        "src/storageServices/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}